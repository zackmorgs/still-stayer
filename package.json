{
  "name": "still-stayer",
  "version": "1.0.0",
  "description": "My go at a wordpress theme for the band [Still Stayer](https://facebook.com/stillstayer).",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all -p watch:js watch:css",
    "build:js": "esbuild ./assets/script.js --bundle --minify --sourcemap --outfile=./assets/script.min.js",
    "watch:js": "esbuild ./assets/script.js --bundle --minify --sourcemap --outfile=./assets/script.min.js --watch",
    "build:css": "esbuild ./style.css --minify --sourcemap --outfile=./style.min.css",
    "watch:css": "esbuild ./style.css --minify --sourcemap --outfile=./style.min.css --watch"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "esbuild": "0.25.5"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
